<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="288.0" prefWidth="434.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="application.EventPlannerController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="56.0" prefWidth="434.0" text="Event Planner ">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
         <font>
            <Font name="Arial" size="39.0" />
         </font>
      </Label>
      <HBox prefHeight="58.0" prefWidth="434.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="156.0" text="Length of Event (in days)">
               <HBox.margin>
                  <Insets left="70.0" top="30.0" />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="eventLengthTextField" prefHeight="26.0" prefWidth="107.0">
               <HBox.margin>
                  <Insets left="10.0" top="30.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <Button fx:id="startButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="254.0" text="Get Started!">
         <VBox.margin>
            <Insets left="90.0" top="30.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
